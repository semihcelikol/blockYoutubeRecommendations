{
  "name": "YoutubeBlockRecommendations",
  "description": "Youtube, block suggestions at the end of the video.",
  "manifest_version": 2,
  "version": "1.0",
  "homepage_url": "http://semihcelikol.com",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "page_action": {
    "default_icon": "icons/offIcon.png",
    "browser_style": true
  },
  
  "permissions": [
    "activeTab",
    "tabs"
  ]

}